<apex:page renderAs="PDF" standardStylesheets="false"  controller="QuoteBidPDFController" lightningStylesheets="false"  applyBodyTag="false" applyHtmlTag="false" showHeader="false" sidebar="false">
    <html>
        <head>
            <style type="text/css" media="print">
                @page {
                    margin-top:20px;
                    margin-bottom:40px;
                    size: 8.5in 11in;
                    margin-left: 0.4in;
                    margin-right: 0.3in;
                    size: portrait;
                    @top-center {
                        content: element(header);
                    }
                    @bottom-left {
                        content: element(footer);
                    }
                }   
                html{
                    margin: 0;
                    padding: 0;
                    width: 100%;
                } 
                body {
                    width: 100%;
                    height: 100%;
                    margin: 0;
                    padding: 0;
                    font: 12px "Helvetica";
                    background-image: url("{!IF(quoteDetails.printQuote.Is_Internal__c,URLFOR($Resource.Quote_Draft_Watermark),URLFOR($Resource.Quote_Empty_Watermark))}");
                    background-repeat: repeat;
                }   
                div.header {
                    display:flex;
                    padding:20px;
                    position: header;
                }
                div.footer {
                    display: block;
                    padding: 2px;
                    position: running(footer);
                }
                .pagenumber:before {
                    content: counter(page);
                }
                .pagecount:before {
                    content: counter(pages);
                }
                .leftHeader{
                    float:right;
                    width: 30%;
                }
                .rightHeader{
                    float:right;
                    width: 35%;
                    margin-top: 2%;
                }
                .middleHeader{
                    float:right;
                    width: 35%;
                    margin-top: 4%;
                }
                .leftFooter{
                    float:left;
                    width: 33%;
                    text-align:left;
                    font: 12pt "Calibri";
                }
                .rightFooter{
                    float:right;
                    width: 33%;
                    text-align:right;
                    font: 12pt "Calibri";
                }
                .middleFooter{
                    float:left;
                    width: 33%;
                    text-align:center;
                    font: 12pt "Calibri";
                }
                .addressLine{
                    text-align:right;
                    font: 8pt "Calibri";
                }
                .titleDiv{
                    padding-left:260px;
                    font: 8.5pt "Calibri";
                    float:left;
                    width:100%;
                }

                /** Table Styles **/

                .tblInfo{padding:5px;width:100%;}
                    .tblInfo .label{
                        font: bold 12px 'Helvetica';
                        width:15%;
                        text-align:left;
                        padding-right:10px;
                        padding-top:8px;
                    }
                    .tblInfo .value{
                        font: 12px 'Helvetica';
                        width:35%;
                        padding-right:10px;
                        padding-top:8px;
                        text-align:left;
                    }
                    .tblInfo .label2{
                        font: bold 12px 'Helvetica';
                        width:20%;
                        text-align:right;
                        padding-right:10px;
                        padding-top:8px;
                    }
                    .tblInfo .intro{
                        font: 12px 'Helvetica';
                        width:100%;
                        padding:10px;
                    }
                    .tblInfo .extraLarge td{
                        font: bold 20px 'Helvetica';
                        text-align:center;
                        padding-bottom:24px;
                    }
                    .tblInfo .signRow td{
                        padding-top:24px;
                    }
                    .tblInfo .signature{
                        border-bottom:0.01em solid black;
                    }
                    .tblInfo li{
                        font: 12px 'Helvetica';
                        padding-bottom:6px;
                    }
                .tblAlts{
                    width:100%;
                    border:1px solid black;
                    margin-bottom:36px;
                    page-break-inside:auto;
                }
                    .tblAlts .group td{
                        background:#a7b1c0; 
                        border:#000000 1px solid;
                        font: bold 12px 'Helvetica';
                        line-height:14px;
                        text-align: center;
                    }
                    .tblAlts .alt td{
                        background:rgb(184, 223, 255);
                        border:#000000 1px solid;
                        font: bold 12px 'Helvetica';
                        text-align:left;
                        line-height:24px;
                    }
                    .tblAlts .altInfo{
                        page-break-inside: avoid;
                    }
                    .tblAlts .altInfo td{
                        background: #b0c4de;
                        border:#000000 1px solid;
                        font: bold 12px 'Helvetica';
                        text-align:center;
                        line-height:24px;
                    }
                    .tblAlts .altInfo2{
                        page-break-inside: avoid;
                    }
                    .tblAlts .altInfo2 td{
                        background: #b0c4de;
                        font: bold 12px 'Helvetica';
                        text-align:center;
                        page-break-inside: avoid;
                        line-height:24px;
                    }
                    .tblAlts .altInfo3{
                        page-break-inside: avoid;
                    }
                    .tblAlts .altInfo3 td{
                        background: #b0c4de;
                        border-top:#000000 1px solid;
                        border-bottom:#000000 1px solid;
                        font: bold 12px 'Helvetica';
                        text-align:left;
                        line-height:24px;
                    }
                    .tblAlts .itemContainer{
                        page-break-inside: avoid;
                    }
                    .tblAlts .itemContainer td{
                        border-bottom:#000000 0.01em solid;
                        padding-bottom:4px;
                        page-break-inside: avoid;
                    }
                    .tblAlts .quantity{
                        text-align:right;
                    }
                    .tblAlts .altTotal td{
                        border-bottom:#000000 0.01em solid;
                        font: bold 12px 'Helvetica';
                        text-align:right;
                        line-height: 16px;
                        padding-right:10px;
                    }

                    .tblAlts .altTotalTop td{
                        font: bold 12px 'Helvetica';
                        text-align:right;
                        padding-bottom:8px;
                        padding-right:10px;
                    }

                    .tblAlts .altTotalBottom td{
                        border-bottom:#000000 0.01em solid;
                        font: bold 12px 'Helvetica';
                        text-align:right;
                        padding-bottom:8px;
                        padding-right:10px;
                    }
                    
                    .tblAlts .altFinish td{
                        font: bold 14px 'Helvetica';
                        padding-right:10px;
                    }

                    .tblAlts .altFinish{
                        margin-bottom:24px;
                    }
                .tblQual{
                    width:100%;
                    margin-top:36px;
                }
                    .tblQual .exp td{
                        font: bold 14px 'Helvetica';
                        text-decoration: underline;
                        text-align:left;
                        padding-bottom:12px;
                    }
                    .tblQual .expItem td{
                        font: 14px 'Helvetica';
                        text-align:left;
                    }
                    .tblQual .large td{
                        font: bold 16px 'Helvetica';
                        text-align:center;
                        padding-bottom:24px;
                    }
                    .tblQual .normal td{
                        font: bold 14px 'Helvetica';
                        text-align:center;
                        padding-bottom:24px;
                    }
                    .tblQual .justified td{
                        font: normal 14px 'Helvetica';
                        text-align:justify;
                        padding-bottom:12px;
                    }
                    .tblQual .center td{
                        font: normal 14px 'Helvetica';
                        text-align:center;
                        padding-bottom:12px;
                    }
                .ftVersion{font: bold 20px 'Helvetica';line-height: 22px;}
            </style>
        </head>
        <body>
            <apex:outputText rendered="{!quoteDetails.printQuote.Form_Type__c == 'Add_Deduct' && quoteDetails.printQuote.Is_Printing_Summary__c}">
                <div class="header">
                    <div style="float:left;margin-left:-25px;">
                        <apex:image url="{!$Resource.Inpro_Header}" width="194" height="124"/>
                    </div>
                    <div class="rightHeader" style="text-align:right;">
                        <apex:outputText rendered="{! !IsCAD}">
                            <div class="addressLine">Corporate Headquarters</div>
                            <div class="addressLine">S80 W18766 Apollo Drive</div>
                            <div class="addressLine">PO Box 406</div>
                            <div class="addressLine">Muskego, WI 53150</div>
                            <div class="addressLine">www.inprocorp.com</div>
                            <div class="addressLine">Nationwide: (800) 222-5556</div>
                            <div class="addressLine">Fax: (888) 715-8407</div>
                        </apex:outputText>
                        <apex:outputText rendered="{!IsCAD}">
                            <div class="addressLine">Inpro Building Products</div>
                            <div class="addressLine">93 Bell Farm Road</div>
                            <div class="addressLine">Suite 109 Barrie</div>
                            <div class="addressLine">Ontario L4M 5G1 2</div>
                            <div class="addressLine">www.inprocorp.com</div>
                            <div class="addressLine">Nationwide: (800) 222-5556</div>
                            <div class="addressLine">Fax: (888) 715-8407</div>
                        </apex:outputText>
                        <div style="font-weight:bold;font-size:14px;"> 
                            <apex:outputText value="{0, date, EEEE, MMMM d, yyyy}">
                                <apex:param value="{!NOW()}" /> 
                            </apex:outputText>
                        </div>
                    </div>
                </div>
                <div class="content" style="margin-top:150px;font-size:14px;font-family:Arial, Helvetica, sans-serif;line-height: 1.5;">
                    <p class="ftVersion" style="text-align: center;">Add/Deduct Summary</p>
                    <p><apex:outputField value="{!quoteDetails.quoteRecord.Introduction__c}"/></p>
                    <p>
                        <table class="tblAlts" style="margin-left:auto;margin-right:auto;width:75%;" cellpadding="0" cellspacing="0">
                            <tr class="altInfo">
                                <td>Alternate</td>
                                <td>Total</td>
                            </tr>
                            <apex:repeat value="{!quoteDetails.groupMap}" var="key">
                                <apex:repeat value="{!key.alternates}" var="alt">
                                        <tr class="itemContainer">
                                            <td>&nbsp;&nbsp;{!alt.alternateName} Total ({!quoteDetails.printQuote.Currency_Code__c}):</td>
                                            <td>&nbsp;&nbsp;
                                                <apex:outputText rendered="{!quoteDetails.printQuote.Is_Taxable__c}" value="${0, number, ###,###,###,##0.00}">
                                                    <apex:param value="{!alt.alternateTotalwTax}" />
                                                </apex:outputText>
                                                <apex:outputText rendered="{! !quoteDetails.printQuote.Is_Taxable__c}" value="${0, number, ###,###,###,##0.00}">
                                                    <apex:param value="{!alt.alternateTotal}" />
                                                </apex:outputText>
                                            </td>
                                        </tr>
                                </apex:repeat>
                            </apex:repeat>
                        </table>
                    </p>
                    <p>
                        <table class="tblAlts" style="margin-left:auto;margin-right:auto;width:75%;" cellpadding="0" cellspacing="0">
                            <tr class="altInfo">
                                <td>Alternate</td>
                                <td>Total</td>
                            </tr>
                            <tr class="itemContainer">
                                <td>
                                    &nbsp;&nbsp;Total in Adds:
                                </td>
                                <td>&nbsp;&nbsp;
                                    <apex:outputText value="${0, number, ###,###,###,##0.00}">
                                        <apex:param value="{!quoteDetails.AddsTotal}" />
                                    </apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    &nbsp;&nbsp;Total in Deducts:
                                </td>
                                <td>&nbsp;&nbsp;
                                    <apex:outputText value="${0, number, ###,###,###,##0.00}">
                                            <apex:param value="{!quoteDetails.DeductsTotal}" />
                                        </apex:outputText>
                                </td>
                            </tr>
                        </table>
                    </p>
                    <p>
                        <apex:outputText rendered="{!quoteDetails.printQuote.Is_Printing_Grand_Total__c}">
                            <table class="tblQual" cellpadding="0" cellspacing="0">
                                <tr class="large"  >
                                    <td style="border-bottom:0.01em solid black; text-align:right;">
                                        Grand Total: ({!quoteDetails.printQuote.Currency_Code__c})&nbsp;
                                        <apex:outputText rendered="{!quoteDetails.printQuote.Is_Taxable__c}">
                                            <apex:outputText value="${0, number, ###,###,###,##0.00}">
                                                <apex:param value="{!quoteDetails.printQuote.Grand_Total_With_Tax__c}" />
                                            </apex:outputText>
                                        </apex:outputText>
                                        <apex:outputText rendered="{! !quoteDetails.printQuote.Is_Taxable__c}">
                                            <apex:outputText value="${0, number, ###,###,###,##0.00}">
                                                <apex:param value="{!quoteDetails.printQuote.Grand_Total__c}" />
                                            </apex:outputText>
                                        </apex:outputText>
                                    </td>
                                </tr>
                            </table>
                        </apex:outputText>
                    </p>
                </div>
                <apex:outputText rendered="{!quoteDetails.printQuote.Is_Printing_Details__c}">
                    <div style="page-break-after:always;"></div>
                </apex:outputText>
            </apex:outputText>
            <apex:outputText rendered="{!(quoteDetails.printQuote.Form_Type__c == 'Add_Deduct' && quoteDetails.printQuote.Is_Printing_Details__c) || quoteDetails.printQuote.Form_Type__c != 'Add_Deduct'}">
                <div class="header">
                    <div class="leftHeader">
        
                    </div>
                    <div class="rightHeader">
                        <apex:outputText rendered="{! !IsCAD}">
                            <div class="addressLine">Corporate Headquarters</div>
                            <div class="addressLine">S80 W18766 Apollo Drive</div>
                            <div class="addressLine">PO Box 406</div>
                            <div class="addressLine">Muskego, WI 53150</div>
                            <div class="addressLine">www.inprocorp.com</div>
                            <div class="addressLine">Nationwide: (800) 222-5556</div>
                            <div class="addressLine">Fax: (888) 715-8407</div>
                        </apex:outputText>
                        <apex:outputText rendered="{!IsCAD}">
                            <div class="addressLine">Inpro Building Products</div>
                            <div class="addressLine">93 Bell Farm Road</div>
                            <div class="addressLine">Suite 109 Barrie</div>
                            <div class="addressLine">Ontario L4M 5G1 2</div>
                            <div class="addressLine">www.inprocorp.com</div>
                            <div class="addressLine">Nationwide: (800) 222-5556</div>
                            <div class="addressLine">Fax: (888) 715-8407</div>
                        </apex:outputText>
                    </div>
                    <div class="middleHeader">
                        <apex:image url="{!$Resource.Inpro_Logo}" width="250" height="100"/>
                    </div>
                </div>
                <div class="footer">
                    <div class="leftFooter">
                        
                    </div>
                    <div class="middleFooter">
                        <b><div>Page <span class="pagenumber"/> of <span class="pagecount"/></div></b>
                    </div>
                    <div class="rightFooter">
                        
                    </div>
                </div>
                <div class="content" style="margin-top:150px;">
                    <div style="text-align: center;">
                        <P class="ftVersion"> 
                            {!IF(quoteDetails.printQuote.Is_Internal__c,'In-House Version ','')}
                            <apex:outputText rendered="{!quoteDetails.printQuote.Is_Submittal_Version__c}">
                                Submittal
                            </apex:outputText>
                            <apex:outputText rendered="{! !quoteDetails.printQuote.Is_Submittal_Version__c}">
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Bid_Format','Bid','')}
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Quote_w_Pricing','Quotation','')}
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Add_Deduct','Add/Deduct','')}
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Quote_w_Lump_Sum_Pricing','Quotation','')}
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Pro_Forma','Pro Forma Invoice','')}
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Pro_Forma_Letter_of_Credit','Pro Forma Invoice For Letter Of Credit','')}
                                {!IF(quoteDetails.printQuote.Form_Type__c == 'Pro_Forma_Wire_Transfer','Pro Forma Invoice For Wire Transfer','')}
                            </apex:outputText>
                        </P>
                    </div> 
                    <table class="tblInfo" cellpadding="0" cellspacing="0" style="padding-bottom:24px;">
                        <!-- Bid Format -->
                        <apex:outputText rendered="{!quoteDetails.printQuote.Form_Type__c == 'Bid_Format'}">
                            <tr>
                                <td class="label">Project:</td>
                                <td class="value">{!quoteDetails.printQuote.Project__c}</td>
                                <td class="label">Date:</td>
                                <td class="value">
                                    <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                        <apex:param value="{!today()}" /> 
                                    </apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <td class="label">Location:</td>
                                <td class="value">{!quoteDetails.printQuote.Location__c}</td>
                                <td class="label">Project Number:</td>
                                <td class="value">{!quoteDetails.opportunityRecord.Project_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value"></td>
                                <td class="label">Quote #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.QuoteNumber}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value"></td>
                                <td class="label">Revision #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Revision_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value"></td>
                                <td class="label">Printed By:</td>
                                <td class="value">{!quoteDetails.printingUser}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value"></td>
                                <td class="label">Document ID:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Inpro_Document_Number__c}</td>
                            </tr>
                            <tr>
                                <td colspan="4" style="padding-top:12px;border-top:0.01em solid black;">
                                    Inpro Corporation is bidding as {!quoteDetails.opportunityRecord.Spec_Status__c} based on the following plans and specifications.
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    Inpro Corporation submits a bid for {!quoteDetails.printQuote.Material__c}.
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4" style="padding-top:12px;">
                                    <apex:outputField value="{!quoteDetails.quoteRecord.Introduction__c}"/>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4" style="padding-top:12px;">
                                    <apex:outputField value="{!quoteDetails.quoteRecord.Quote_Estimating_Notes__c}"/>
                                </td>
                            </tr>
                        </apex:outputText>
                        <!-- Quote w/Pricing -->
                        <apex:outputText rendered="{!quoteDetails.printQuote.Form_Type__c == 'Quote_w_Pricing' || quoteDetails.printQuote.Form_Type__c == 'Add_Deduct' || quoteDetails.printQuote.Form_Type__c == 'Quote_w_Lump_Sum_Pricing'}">
                            <tr>
                                <td class="label">To:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.Name}</td>
                                <td class="label">Date:</td>
                                <td class="value">
                                    <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                        <apex:param value="{!today()}" /> 
                                    </apex:outputText>
                                </td>
                            </tr>
                            <tr>
                                <td class="label">ATTN:</td>
                                <td class="value">{!quoteDetails.printQuote.Contact_Name__c} {!quoteDetails.printQuote.Contact_Phone__c}</td>
                                <td class="label">Project Number:</td>
                                <td class="value">{!quoteDetails.opportunityRecord.Project_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.printQuote.Contact_Email__c}</td>
                                <td class="label">Quote #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.QuoteNumber}</td>
                            </tr>
                            <tr>
                                <td class="label">Fax:</td>
                                <td class="value">{!quoteDetails.printQuote.Fax__c}</td>
                                <td class="label">Revision #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Revision_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label">Project:</td>
                                <td class="value">{!quoteDetails.printQuote.Project__c}</td>
                                <td class="label">Customer #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.Customer_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label">Location:</td>
                                <td class="value">{!quoteDetails.printQuote.Location__c}</td>
                                <td class="label">Printed By:</td>
                                <td class="value">{!quoteDetails.printingUser}</td>
                            </tr>
                            <tr>
                                <td class="label">Sales Rep:</td>
                                <td class="value">{!quoteDetails.opportunityRecord.Owner.Name}</td>
                                <td class="label">Document ID:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Inpro_Document_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.opportunityRecord.Owner.Email}</td>
                                <td class="label"></td>
                                <td class="value"></td>
                            </tr>
                            <tr>
                                <td colspan="4" style="padding-top:12px;">
                                    <apex:outputField value="{!quoteDetails.quoteRecord.Introduction__c}"/>
                                </td>
                            </tr>
                            <apex:outputText rendered="{!quoteDetails.printQuote.Form_Type__c != 'Add_Deduct'}">
                                <tr>
                                    <td colspan="4" style="padding-top:12px;">
                                        <apex:outputField value="{!quoteDetails.quoteRecord.Quote_Estimating_Notes__c}"/>
                                    </td>
                                </tr>
                            </apex:outputText>
                        </apex:outputText>
                        <apex:outputText rendered="{!quoteDetails.printQuote.Form_Type__c == 'Pro_Forma'}">
                            <tr>
                                <td class="label">Date:</td>
                                <td class="value">
                                    <apex:outputText value="{0,date,MM'/'dd'/'yyyy}">
                                        <apex:param value="{!today()}" /> 
                                    </apex:outputText>
                                </td>
                                <td class="label">Customer #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.Customer_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label">PO Number:</td>
                                <td class="value">{!quoteDetails.printQuote.PO_Number__c}</td>
                                <td class="label">Project Number:</td>
                                <td class="value">{!quoteDetails.opportunityRecord.Project_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label">Sold To:</td>
                                <td class="value">ATTN: {!quoteDetails.quoteRecord.Primary_Bidder__r.Name}</td>
                                <td class="label">Quote #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.QuoteNumber}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.Name}</td>
                                <td class="label">Revision #:</td>
                                <td class="value">{!quoteDetails.quoteRecord.Revision_Number__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.BillingStreet}</td>
                                <td class="label">Ship To:</td>
                                <td class="value">{!quoteDetails.printQuote.Ship_To_Name__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.BillingCity}, {!quoteDetails.quoteRecord.Primary_Bidder__r.BillingState} {!quoteDetails.quoteRecord.Primary_Bidder__r.BillingPostalCode}</td>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.printQuote.Ship_To_Street__c}</td>
                            </tr>
                            <tr>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.quoteRecord.Primary_Bidder__r.BillingCountry}</td>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.printQuote.Ship_To_City__c}, {!quoteDetails.printQuote.Ship_To_State__c}  {!quoteDetails.printQuote.Ship_To_Postal_Code__c}</td>
                            </tr>
                            <tr>
                                <td class="label">Sold To Phone:</td>
                                <td class="value">{!quoteDetails.printQuote.Contact_Phone__c}</td>
                                <td class="label"></td>
                                <td class="value">{!quoteDetails.printQuote.Ship_To_Country__c}</td>
