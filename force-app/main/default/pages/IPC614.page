        <style type="text/css">
            @page {
                size: 8.5in 11in portrait;
                margin-top: 0.2in;
                margin-bottom: 0.2in;
                margin-left: 0.4in;
                margin-right: 0.3in;
                background-repeat: no-repeat;
            background-position: center;

            @top-center {
            content: element(header);
            }
            @bottom-left {
            content: element(footer);
            }
            }
            
            div.header {
            padding: 10px;
            position: running(header);
            }
            }
        </style>
