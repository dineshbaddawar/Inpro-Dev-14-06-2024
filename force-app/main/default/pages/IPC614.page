                            </apex:outputText>
                            <apex:outputText rendered="{!quoteLine.Item_Number__c != null && NOT(CONTAINS(quoteLine.Item_Number__c, 'adh')) && NOT(CONTAINS(quoteLine.Item_Number__c, 'dmt'))}">
