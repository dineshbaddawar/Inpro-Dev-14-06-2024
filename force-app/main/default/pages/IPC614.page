                            <apex:outputText rendered="{!quoteLine.Item_Number__c != null && CONTAINS(quoteLine.Item_Number__c, 'adh')}">
                                Adhesive
                            </apex:outputText>
                            <apex:outputText rendered="{!quoteLine.Item_Number__c != null && CONTAINS(quoteLine.Item_Number__c, 'dmt')}">
