                    background-repeat: repeat;
                }
                @page {
                margin-top:20px;
                margin-bottom:40px;
                size: 8.5in 11in;
                margin-left: 0.3in;
                margin-right: 0.3in;
                size: portrait;
                @top-center {
                content: element(header);
                }
                @bottom-left {
                content: element(footer);
                }
                }
                html{
                margin: 0;
                padding: 0;
                width: 100%;
                }
                body {
                width: 100%;
                height: 100%;
                margin: 0;
                padding: 0;
                font: 8.5pt "Calibri";
                }                   
                div.header {
                padding:20px;
                position: header;
                }
                div.footer {
                display: block;
                padding: 2px;
                position: running(footer);
                }
                .pagenumber:before {
                content: counter(page);
                }
                .pagecount:before {
                content: counter(pages);
                }
                
                table{
                
                border-collapse: collapse;
                }
                
                #leftHeader{
                float:right;
                width: 25%;
                }
                
