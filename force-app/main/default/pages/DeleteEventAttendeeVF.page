<apex:page standardController="LunchAndLearnEvent__c" extensions="DeleteEventAttendee"  showHeader="false" sidebar="false" >
    <apex:form >
     
       <apex:sectionHeader title=" {!lunchlearnname}" /> 
        <apex:pageBlock Title="{!IF(listMemberWrapper != NULL && listMemberWrapper.size > 0,'Attendees of the Event','There are no Attendees at this Event to Delete')} ">
           <apex:pageBlockButtons >
               <apex:commandButton value="Delete Selected" action="{!processSelected}" />
               <apex:commandButton value="Cancel" action="{!cancel}" />
            </apex:pageBlockButtons>
                <apex:pageBlockTable value="{!listMemberWrapper}" var="Member" rendered="{!IF(listMemberWrapper != NULL && listMemberWrapper.size > 0,true,false)}">                    
                   <apex:column >
                       <apex:inputCheckbox value="{!Member.selected}"/>
                   </apex:column>
                   <apex:column value="{!Member.CampaignMe.id}"/>
                   <apex:column value="{!Member.CampaignMe.name}"/>
                   <apex:column value="{!Member.CampaignMe.FirstName__c}"/>
                   <apex:column value="{!Member.CampaignMe.LastName__c}"/>
                   <apex:column value="{!Member.CampaignMe.Email__c}"/>
                   <apex:column value="{!Member.CampaignMe.LunchAndLearn_AIA_Number__c}"/>
                </apex:pageBlockTable>
          
        </apex:pageBlock>
    </apex:form>
</apex:page>